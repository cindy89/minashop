<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping package="com.minashop.entity">
     <class name="Order" table="t_order">
         <id name="id" type="java.lang.Integer">
         	<column name="id"></column>
         	<generator class="increment"></generator>
         </id>
         <property name="cname" column="c_name" type="java.lang.String">
         </property>
         <property name="zip" type="java.lang.String">
         </property>
         <property name="addr" type="java.lang.String">
         </property>
         <property name="phone" type="java.lang.String">
         </property>
         <property name="email" type="java.lang.String">
         </property>
         <property name="bNo" column="b_no" type="java.lang.String">
         </property>
         <property name="bNum" column="b_num" type="java.lang.String">
         </property>
     </class>
 </hibernate-mapping> 